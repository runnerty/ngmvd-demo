{
  "id": "check-my-site",
  "name": "Checks my site every minute",
  "triggers":[
    {
      "id":"schedule_dafault",
      "schedule_interval":"*/1 * * * *"
    }
  ],
  "notifications": {
    "on_start": [
      {
        "id": "console_default",
        "message": "@GETDATE('YYYY-MM-DD HH:mm:ss') START OF THE CHAIN: @GV(MY_VALUE_ONE)"
      }
    ],
    "on_end": [
      {
        "id": "console_default",
        "message": "@GETDATE('YYYY-MM-DD HH:mm:ss') END OF THE CHAIN: @GV(MY_VALUE_ONE)"
      }
    ]
  },
  "processes": [
    {
      "id": "SEND_WELLCOME_MAIL",
      "name": "Sends the wellcome message to the user",
      "depends_process": {},
      "exec": {
        "id":"check_online_service_default",  
        "hostname": "http://www.google.es",
        "check_contains": "Voy a tener suelte"
      },
      "notifications": {
        "on_fail": [
          {
            "id": "console_default",
            "message": "My site is offline!!!"
          },
          {
            "id": "telegram_default",
            "message": "My site is offline!!!"
          }
        ]
      }
    }
  ]
}
